{
  "rewrites": [
    {
      "source": "/password-challenge(.*)",
      "destination": "https://sense-of-security.vercel.app$1"
    },
    {
      "source": "/leaderboard(.*)",
      "destination": "https://sense-of-security.vercel.app/leaderboard$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}